type Dog{
    id:ID!
    name:String!
    breed:String!
    origin:String!
}

type Query{
    findDogBreeds:[String]
    findDogBreedById:String
    findAllDogNames:[String]
    # or we can have a general query and the user can pick the exact value
    # findAllDogs: [Dog]!
    # findDogById: Dog!

}

type Mutations{
    deleteDogBreed(breed:String!):Boolean
    updateDogName(id:ID!,newName:String):Dog!
}